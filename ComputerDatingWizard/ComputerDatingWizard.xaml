<Window x:Class="Petzold.ComputerDatingWizard.ComputerDatingWizard"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="Computer Dating Wizard" Height="300" Width="300"
	WindowStartupLocation="CenterScreen"
	ResizeMode="NoResize">
	<Grid>
		<Grid.RowDefinitions>
			<RowDefinition Height="Auto" />
			<RowDefinition Height="*" />
		</Grid.RowDefinitions>
		<TextBlock Grid.Row="0" Margin="12" FontSize="16"
			FontStyle="Italic" HorizontalAlignment="Center">
		Computer dating wizard
		</TextBlock>
		<Frame Grid.Row="1" Name="frame" NavigationUIVisibility="Hidden"
			Padding="6" />
	</Grid>
</Window>