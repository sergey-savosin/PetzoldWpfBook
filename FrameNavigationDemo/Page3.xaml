<Page x:Class="Petzold.FrameNavigationDemo.Page3"
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	Title="Page3">
		<Grid>
			<Grid.RowDefinitions>
				<RowDefinition Height="50*" />
				<RowDefinition Height="50*" />
			</Grid.RowDefinitions>
			<TextBox Grid.Row="0" MinWidth="2in" Margin="48"
				HorizontalAlignment="Center" VerticalAlignment="Top" />
			<TextBlock Grid.Row="1" Margin="48" HorizontalAlignment="Right" VerticalAlignment="Bottom">
			Go to back to
				<Hyperlink NavigateUri="Page1.xaml">
				Page 1
				</Hyperlink>
			</TextBlock>
		</Grid>
</Page>